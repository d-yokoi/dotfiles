if has("syntax")
  syntax on
endif

colorscheme desert

set autoread
set ambiwidth=double
set ttyfast
set t_Co=256
set number
set title
set list
set ambiwidth=double
set expandtab
set tabstop=2
set shiftwidth=2
set smartindent
set autoindent
set cursorline
set backspace=indent,eol,start

inoremap <silent> jj <ESC>
